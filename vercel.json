{
    "version": 2,
    "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [{
        "src": "/(.*)",
        "dest": "src/server.ts"
    }]
}